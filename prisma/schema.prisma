generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model BlockLibraryItem {
  id String @id @default(cuid())

  blockName   String
  code        String @unique
  tplFilename String
  nameLabel   String
  description String

  tab  String
  icon String

  hasRepeater Boolean

  input Json

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
